{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "settings": {
        "import/resolver": "webpack"
    },
    "rules": {
        "jsx-a11y/label-has-associated-control": 0
    },
    "plugins": ["jest"],
    "env": {
        "jest/globals": true,
        "browser": true, // document will work
        "node": true
    }
}